<!doctype html>
<html>
<head>
  <meta charset='utf-8' />
  <title><%- options.name %> <%- options.version %> | Documentation</title>
  <meta name='viewport' content='width=device-width,initial-scale=1'>
  <link href='assets/bass.css' type='text/css' rel='stylesheet' />
  <link href='assets/style.css' type='text/css' rel='stylesheet' />
  <link href='assets/github.css' type='text/css' rel='stylesheet' />
</head>
<body class='documentation'>
  <div class='container'>
    <div class='clearfix md-mxn2'>
      <div class='fixed md-show fix-3 overflow-auto max-height-100'>
        <div class='py1 px2'>
          <h3 class='mb0 no-anchor'><%- options.name %></h3>
          <div class='mb1'><code><%- options.version %></code></div>
          <input
            placeholder='Filter'
            id='filter-input'
            class='col12 block field'
            type='text' />
          <div id='toc'>
            <% docs.forEach(function(doc) { %>
              <a
                href='#<%=permalink(doc.path)%>'
                class='block bold'>
                <%- doc.name %>
              </a>
              <% doc.members.static && doc.members.static.forEach(function(member) { %>
                <a
                  href='#<%=permalink(member.path)%>'
                  class='regular block'>
                  <%- member.name %>
                </a>
              <% }) %>
              <% doc.members.instance && doc.members.instance.forEach(function(member) { %>
                <a
                  href='#<%=permalink(member.path)%>'
                  class='regular block'>
                  <%- member.name %>
                </a>
              <% }) %>
              <% doc.members.events && doc.members.events.forEach(function(member) { %>
                <a
                  href='#<%=permalink(member.path)%>'
                  class='regular block'>
                  <code>â“” <%- member.name %></code>
                </a>
              <% }) %>
            <% }) %>
          </div>
        </div>
      </div>
      <div class='fix-margin-3'>
        <div class='px2'>
          <% docs.forEach(function(s) { %>
            <div class='py1'><%=section({ section: s })%></div>
          <% }) %>
        </div>
      </div>
    </div>
  </div>
  <script src='assets/anchor.js'></script>
  <script src='assets/site.js'></script>
</body>
</html>
