<section class='py2 clearfix'>
  <h2 id='<%- permalink(section.path) %>' class='mt0'>
    <%- section.name %><span class='gray'><%- formatParameters(section) %></span>
  </h2>
  <% if (section.augments) { %>
    <p>
      Extends
      <% if (augments) { %>
        <%= autolink(section.name) %>
      <% } %>
    </p>
  <% }%>
  <% if (section.context.github) { %>
    <p><a href='<%- context.github%>'><code><%- context.path %></code></a></p>
  <% } %>
  <% if (section.version) { %><div>Version: <%- section.version %></div><% }%>
  <% if (section.license) { %><div>License: <%- section.license %></div><% }%>
  <% if (section.author) { %><div>Author: <%- section.author %></div><% }%>
  <% if (section.copyright) { %><div>Copyright: <%- section.copyright %></div><% }%>
  <% if (section.since) { %><div>Since: <%- section.since %></div><% }%>
  <%= md(section.description) %>
  <% if (section.params) { %>
    <h4>Parameters</h4>
    <ul class='suppress-p-margin'>
      <% section.params.forEach(function(param) { %>
        <li><%= formatType(param) %> <strong><%- param.name%></strong>
          <% if (param.default) { %>
            (default <code><%- param.default %></code>)
          <% } %>
          :
          <div class='force-inline'>
            <%= md(param.description) %>
          </div>
          <% if (param.properties) { %>
            <ul>
              <% param.properties.forEach(function(property) { %>
                <li><%= formatType(property) %> <%- property.name %>
                  <% if (property.default) { %>
                    (default <code><%- property.default %></code>)
                  <% } %>
                  <%- md(property.description) %></li>
              <% }) %>
            </ul>
          <% } %>
        </li>
      <% }) %>
    </ul>
  <% } %>

 <% if (section.properties) { %>
    <h4>Properties</h4>
    <ul class='suppress-p-margin'>
      <% section.properties.forEach(function(property) { %>
        <li><%= formatType(property.type) %> <strong><%- property.name%></strong>
          <% if (property.default) { %>
            (default <code><%- property.default %></code>)
          <% } %>
          <% if (property.description) { %>
            :
            <div class='force-inline'>
              <%= md(property.description) %>
            </div>
	  <% } %>
        </li>
        <% if (section.properties) { %>
          <ul>
            <% property.properties.forEach(function(property) { %>
              <li><code><%- name%></code> <%= formatType(property.type) %>
                <% if (property.default) { %>
                  (default <code><%- property.default %></code>)
                <% } %>
                <% md(property.description) %></li>
            <% }) %>
          </ul>
        <% } %>
      <% }) %>
    </li>
  <% } %>
  <% if (section.returns) { %>
     <% section.returns.forEach(function(ret) { %>
       <h4>Returns</h4>
       <code><%= formatType(ret) %></code>
       <% if (ret.description) { %>:
	 <div class='force-inline'>
	   <%= md(ret.description) %>
	 </div>
       <% }%>
     <% }) %>
   <% } %>
  <% if (section.throws) { %>
    <h4>Throws</h4>
    <ul>
      <% section.throws.forEach(function(throws) { %>
        <li><%= formatType(throws) %>:
	  <div class='force-inline'><%= md(throws.description) %></div></li>
      <% }); %>
    </ul>
  <% } %>

  <% if (section.examples) { %>
    <h4>Examples</h4>
    <% section.examples.forEach(function(example) { %>
      <% if (example.caption) { %><p><%= md(example.caption) %></p><% } %>
      <pre class='overflow-auto'><%= highlight(example.description) %></pre>
    <% }) %>
  <% } %>

  <% if (section.members.static.length) { %>
    <h4>Static members</h4>
    <% section.members.static.forEach(function(static) { %>
    <% }) %>
  <% } %>
